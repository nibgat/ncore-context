{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.options": {
    "extensions": [".html", ".js", ".ts", ".tsx", ".jsx"]
  },
  "eslint.validate": ["javascript", "html", "javascriptreact", "typescriptreact", "typescript"],
  "files.eol": "auto"
}
