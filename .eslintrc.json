{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "object-curly-newline": ["error", "always"],
    "indent": ["error", 4],
    "semi": "error"
  }
}
